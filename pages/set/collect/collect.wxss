/* pages/set/collect/collect.wxss */
page{
  background-color: #f8f8f8;
}
.header {
  position: relative;
  display: flex;
  widows: 100%;
  height: 40px;
  background-color: #f06c7a;
}
.icon-left {
  position: absolute;
  margin: 7px;
}
.font {
  height: 40px;
  line-height: 40px;
  margin: 0 auto;
  font-size: 30rpx;
  color: rgb(255, 255, 255);
}
.van-card__header {
  width: 100%;
  height: 140px;
  background-color: rgb(112, 21, 21);
  padding: 10px;
}
.van-card {
  padding: 0 !important;
}
.van-card__thumb {
  width: 100%;
  height: 200px;
}
.van-card__img {
  width: 140px !important;
  height: 140px !important;
  border-radius: 10% !important;
}
.van-card__content {
  margin: 15px;
  margin-left: 50px;
}

