/* pages/set/coupon/coupon.wxss */
.pic-box {
  width: 212px;
  height: 168px;
  margin: 100px auto;
}
.pic {
  width: 100%;
  height: 168px;
}
.box1 {
  box-shadow: 3px 0px 10px #b3a8a8;
  margin: 10px auto;
  border-radius: 10px;
  border: 1px solid #ccc;
  width: 95%;
  height: 100px;
}
.left {
  float: left;
  width: 150px;
  height: 100px;
}
.money {
  color: red;
  line-height: 100px;
  text-align: center;
  font-size: 30px;
  font-weight: bold;
}
.right {
  float: right;
  width: 230px;
  height: 100px;
}
.title {
  color: black;
  line-height: 100px;
  text-align: center;
  font-size: 20px;
}